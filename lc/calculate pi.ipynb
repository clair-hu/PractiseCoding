    "import random\n",
    "seed = 1\n",
    "random.seed(seed)\n",
    "num_iter = 10\n",
    "inner = 0\n",
    "for i in range(num_iter):\n",
    "    x = random.uniform(-1,1)\n",
    "    y = random.uniform(-1,1)\n",
    "    if x**2 + y**2 <= 1:\n",
    "        inner += 1\n",
    "pi = 4 * inner / num_iter\n",
    "print(pi)"
   ]
