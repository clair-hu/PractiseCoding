   "source": [
    "def integerBreak(n):\n",
    "    dp = [1] * (n+1)\n",
    "    for num in range(3, n+1):\n",
    "        for i in range(2, num):\n",
    "            dp[num] = max(dp[num], max(i * (num - i), i * dp[num-i]))\n",
    "    return dp[n]\n"
   ]
  },
